<div class="photo-card col-sm-6 col-md-4 col-xl-3 col-xxl-2">
  {{!-- previewURL webformatURL largeImageURL--}}
  <a class="galLnk">
    <img 
      class="galImg lazyload" 
      loading="lazy" 
      data-src="{{previewURL}}" 
      alt="image id={{id}}"
      height="200" 
      data-source="{{largeImageURL}}" 
      data-id="{{id}}"  />
    {{!-- <img src="{{webformatURL}}" alt="image id={{id}}" /> --}}
  </a>

  <div class="stats">
    {{!-- <span>id {{id}}</span> --}}
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      {{likes}}
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
      {{views}}
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      {{comments}}
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      {{downloads}}
    </p>
  </div>
</div>
